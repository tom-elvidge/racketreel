{
    "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 2,
    "generator-cli": {
      "version": "6.0.1",
      "useDocker": true,
      "generators": {
        "server": {
          "generatorName": "aspnetcore",
          "output": ".",
          "glob": "matches.oas3.json",
          "additionalProperties": {
            "aspnetCoreVersion": "6.0",
            "newtonsoftVersion": "6.0.8",
            "sourceFolder": "",
            "buildTarget": "library",
            "classModifier": "abstract",
            "generateBody": false,
            "operationResultTask": true,
            "packageAuthors": "Tom Elvidge",
            "packageDescription": "Generated server library for the Matches service",
            "packageName": "RacketReel.Services.Matches.Generated.Server",
            "packageTitle": "Matches Server Library",
            "packageVersion": "0.1.0"
          }
        }
      }
    }
  }